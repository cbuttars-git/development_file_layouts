<Patterns xmlns="urn:schemas-jetbrains-com:member-reordering-patterns">
  <TypePattern>

    <!-- Protected Constant Block Start -->
    <Entry DisplayName="Protected bool constant" Priority="10">
      <Entry.Match>
        <And>
          <Access Is="Protected"/>
          <Type Is="bool"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name/>
      </Entry.SortBy>
    </Entry>

