<Patterns xmlns="urn:schemas-jetbrains-com:member-reordering-patterns">
  <TypePattern>
    <!-- Private Constant Block Start -->
    <Entry DisplayName="Private Constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="Private"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Private Constant Block End -->
  </TypePattern>
</Patterns>