<Patterns xmlns="urn:schemas-jetbrains-com:member-reordering-patterns">
  <TypePattern>

    <!-- Private Protected Constant Block Start -->
    <Entry DisplayName="PrivateProtected bool constant" Priority="10">
      <Entry.Match>
        <And>
          <Access Is="PrivateProtected"/>
          <Type Is="bool"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name/>
      </Entry.SortBy>
    </Entry>

