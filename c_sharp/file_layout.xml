<Patterns xmlns="urn:schemas-jetbrains-com:member-reordering-patterns">
  <TypePattern DisplayName="Default Pattern" Priority="1" RemoveRegions="All">
    <TypePattern.Match>
      <Or>
        <Kind Is="Class"/>
        <Kind Is="Record" />
        <Kind Is="Interface" />
        <Kind Is="Enum" />
        <Kind Is="Struct"/>
      </Or>
    </TypePattern.Match>

    <!-- 0010 Public Constant Block Start -->
    <Entry DisplayName="Public constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="Public"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
         <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Public Constant Block End -->

    <!-- 0020 Protected Constant Block Start -->
    <Entry DisplayName="Protected constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="Protected"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
         <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Protected Constant Block End -->

    <!-- 0030 Protected Internal Constant Block Start -->
    <Entry DisplayName="ProtectedInternal constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="ProtectedInternal"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
         <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Protected Internal Constant Block End -->

    <!-- 0040 Internal Constant Block Start -->
    <Entry DisplayName="Internal constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="Internal"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
         <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Internal Constant Block End -->
    
    <!-- 0050 Private Protected Constant Block Start -->
    <Entry DisplayName="Private Protected constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="PrivateProtected"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
         <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Private Protected Constant Block End -->

    <!-- 0060 Private Constant Block Start -->
    <Entry DisplayName="Private constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="Private"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
         <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Private Constant Block End -->

    <!--  0070 Public Field Block Start -->
    <Entry DisplayName="Public Field" Priority="99">
      <Entry.Match>
        <And>
          <Access Is="Public"/>
          <Kind Is="Field"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Public Field Block End -->

    <!-- 0080 Protected Field Block Start -->
    <Entry DisplayName="Protected Field" Priority="99">
      <Entry.Match>
        <And>
          <Access Is="Protected"/>
          <Kind Is="Field"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Protected Field Block End -->
    
  </TypePattern>
</Patterns>