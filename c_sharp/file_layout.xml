<Patterns xmlns="urn:schemas-jetbrains-com:member-reordering-patterns">
  <TypePattern DisplayName="Default Pattern" Priority="1" RemoveRegions="All">
    <TypePattern.Match>
      <Or>
        <Kind Is="Class"/>
        <Kind Is="Record" />
        <Kind Is="Interface" />
        <Kind Is="Enum" />
        <Kind Is="Struct"/>
      </Or>
    </TypePattern.Match>

    <!-- Public Constant Block Start -->
    <Entry DisplayName="Public constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="Public"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name/>
      </Entry.SortBy>
    </Entry>
    <!-- Public Constant Block End -->

    <!-- Protected Constant Block Start -->
    <Entry DisplayName="Protected constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="Protected"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name/>
      </Entry.SortBy>
    </Entry>
    <!-- Protected Constant Block End -->

    <!-- Protected Internal Constant Block Start -->
    <Entry DisplayName="ProtectedInternal constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="ProtectedInternal"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name/>
      </Entry.SortBy>
    </Entry>
    <!-- Protected Internal Constant Block End -->

    <!-- Private Protected Constant Block Start -->
    <Entry DisplayName="Private Protected constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="PrivateProtected"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name/>
      </Entry.SortBy>
    </Entry>
    <!-- Private Protected Constant Block End -->

    <!-- Private Constant Block Start -->
    <Entry DisplayName="Protected bool constant" Priority="100">
      <Entry.Match>
        <And>
          <Access Is="Private"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name/>
      </Entry.SortBy>
    </Entry>
    <!-- Private Constant Block End -->
    
  </TypePattern>
</Patterns>