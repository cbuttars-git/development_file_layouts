<Patterns xmlns="urn:schemas-jetbrains-com:member-reordering-patterns">
  <TypePattern>
    
    <!-- Public Constant Block Start -->
    <Entry DisplayName="Public bool constant" Priority="10">
      <Entry.Match>
        <And>
          <Access Is="Public"/>
          <Type Is="bool"/>
          <Kind Is="Constant"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name/>
      </Entry.SortBy>
    </Entry>
  
