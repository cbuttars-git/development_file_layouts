<Patterns xmlns="urn:schemas-jetbrains-com:member-reordering-patterns">
  <TypePattern>
    <!-- Public Readonly Field Block Start -->
    <Entry DisplayName="Public Readonly Field" Priority="115">
      <Entry.Match>
        <And>
          <Access Is="Public"/>
          <Readonly/>
          <Kind Is="Field"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Public Readonly Field Block End -->
  </TypePattern>
</Patterns>