<Patterns xmlns="urn:schemas-jetbrains-com:member-reordering-patterns">
  <TypePattern>
    <!-- Private Protected Field Block Start -->
    <Entry DisplayName="Private Protected Field" Priority="99">
      <Entry.Match>
        <And>
          <Access Is="PrivateProtected"/>
          <Kind Is="Field"/>
        </And>
      </Entry.Match>
      <Entry.SortBy>
        <Name Direction="Descending"/>
      </Entry.SortBy>
    </Entry>
    <!-- Private Protected Field Block End -->
  </TypePattern>
</Patterns>